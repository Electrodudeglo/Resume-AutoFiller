{
    "manifest_version": 3,
    "name": "Ai AutoFiller",
    "description": "Utilize AI to fill in forms automatically",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"  
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
     "options_page": "options.html",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "icons": {
   "16": "helldivers.png"
   
 }
  }